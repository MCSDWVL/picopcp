pico-8 cartridge // http://www.pico-8.com
version 5
__lua__
actors={}
player_movespeed = 1
pl = nil

function create_actor(x, y, spr, frames, dx, dy, damping)
  a={}
  a.x = x
  a.y = y
  a.spr = spr
  a.frame = 0
  a.frames = frames
  a.dx = dx
  a.dy = dy
  a.damping = damping

  add(actors,a)

  return a
end

function draw_actor(a)
  -- todo: draw sprite
  -- spr(a.spr + a.frame, sx, sy)
  circfill(a.x,a.y,3,spr)
end

function update_player(a)
  if (btn(0)) then a.dx-=player_movespeed end
  if (btn(1)) then a.dx+=player_movespeed end
  if (btn(2)) then a.dy-=player_movespeed end
  if (btn(3)) then a.dy+=player_movespeed end
end

function apply_movement(a)
  a.x += a.dx
  a.y += a.dy

  -- damping
  a.dx *= a.damping
  a.dy *= a.damping

  -- check if npc has moved "off screen"
  if (a.y<-1 and a ~= pl) then
    del(actors,a)
  end
end

function _init()
  pl = create_actor(70, 90, 0, 1, 0, 0, .9)
end

frames_til_next_enemy = 30
function update_frames_til_next_enemy()
  frames_til_next_enemy -= 1
  if (frames_til_next_enemy <= 0) then
    x = flr(rnd(127))
    y = 130
    create_actor(x, y, 1, 0, 0, -1, 1)
    frames_til_next_enemy = 30
  end
end

function _update()
  -- player movement
  update_player(pl)

  -- apply all the forces
  foreach(actors,apply_movement)

  -- check for adding enemies
  update_frames_til_next_enemy()
end


function _draw()
  rectfill(0,0,127,127,5)
  print(frames_til_next_enemy, 50,50, 3)
  print(#actors, 70,70,2)
  foreach(actors,draw_actor)
end
